{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "modal": [
        "dist/modal"
      ],
      "modal/*": [
        "dist/modal/*"
      ],
      "core": [
        "dist/core"
      ],
      "core/*": [
        "dist/core/*"
      ],
      "click-outside": [
        "dist/click-outside"
      ],
      "click-outside/*": [
        "dist/click-outside/*"
      ],
      "on-dom-activity": [
        "dist/on-dom-activity"
      ],
      "on-dom-activity/*": [
        "dist/on-dom-activity/*"
      ],
      "spinner": [
        "dist/spinner"
      ],
      "spinner/*": [
        "dist/spinner/*"
      ],
      "table": [
        "dist/table"
      ],
      "table/*": [
        "dist/table/*"
      ],
      "input-select": [
        "dist/input-select"
      ],
      "input-select/*": [
        "dist/input-select/*"
      ],
      "dropdown": [
        "dist/dropdown"
      ],
      "dropdown/*": [
        "dist/dropdown/*"
      ],
      "keyboard-events-emitter": [
        "dist/keyboard-events-emitter"
      ],
      "keyboard-events-emitter/*": [
        "dist/keyboard-events-emitter/*"
      ]
    }
  }
}